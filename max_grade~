#!/usr/bin/env python
import sys


def main():
	try:
    fp = open(sys.argv[1],"r")

    students = []
    for line in list(fp)
        tokens = line.strip().split(',')
        student = {
                    'test': tokens[0],
                    'liceu': tokens[1],
                    'nume': tokens[2],
                    'nota': int(tokens[3])
                    }
        students.append(student)
    print students 

if __name__ == "__main__"
    sys.exit(main())
